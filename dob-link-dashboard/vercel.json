{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@dob-api-url",
    "NEXT_PUBLIC_SUPABASE_URL": "@dob-supabase-url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@dob-supabase-anon-key"
  }
} 